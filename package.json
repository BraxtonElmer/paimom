{
  "name": "paimom",
  "version": "1.0.0",
  "description": "Ultimate Genshin Impact Discord Bot with modern interactions (TypeScript)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node --watch dist/index.js",
    "dev:build": "tsc --watch",
    "deploy": "tsc && node dist/deploy-commands.js",
    "db:migrate": "tsc && node dist/database/migrate.js",
    "db:seed": "tsc && node dist/database/seed.js",
    "db:reset": "tsc && node dist/database/reset.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "discord",
    "bot",
    "genshin-impact",
    "discord-bot",
    "genshin-helper",
    "typescript"
  ],
  "author": "BraxtonElmer",
  "license": "GPL-3.0",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/luxon": "^3.3.7",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
